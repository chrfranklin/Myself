<div class="row">
  <div class="span6 offset3">
    <h1>Users List</h1>
    <table class="table">
    <tr>
      <td>ID</td>
      <td>用户名</td>
      <td>Email</td>
    </tr>
    <% @user.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
    <tr>
    <% end %>
    </table>
  </div>
</div>

