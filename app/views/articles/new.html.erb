<h1>Edit编辑</h1>
<div class="row">
  <div class="span6 offset3">
  <%= form_for @article do |f| %>
    <%= f.label :title %>
    <%= f.text_field :heading %>
    
    <%= f.label :content %>
    <%= f.text_field :content %>
        
    <%= f.submit "Create my article", class: "btn btn-large btn-primary" %>
  <%end%>
  </div>
</div>
