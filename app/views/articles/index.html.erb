<div class="row">
  <div class="span6 offset3">
  <h1 class="text-info">寂静唠叨</h1>
  <table class="table table-bordered">
    <% @article.each do |a| %>
    <tr>
      <td><%=a.id%></td>
      <td><%= link_to a.heading, a %></td>
      <%if signed_in?%>
